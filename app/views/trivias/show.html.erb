<div class="well">
  <h1>Trivias Game</h1>
    <p><%= @trivia_question.question %></p>
    <%= form_tag '/', method: :get do %>
      <% @trivia_answers.shuffle.each do |answer| %>
        <input type="submit" class="btn btn-primary" name="answer" value="<%= answer.answer %>">
      <% end %>
    <% end %>
</div>
